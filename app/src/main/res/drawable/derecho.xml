<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke android:color="#CECECE" android:width="1dp"></stroke>
            <solid android:color="#E8FFE6"></solid>
            <padding android:bottom="2dp"
                android:left="5dp"
                android:right="5dp"
                android:top="2dp"/>
